    "vue",
    "vuejs",
    "plugin",
    "textarea",
    "size",
    "auto",
    "autosize",
    "height"
  ],
  "author": {
    "name": "codefarm.dev",
    "email": "codefarm.bs@gmail.com"
  },
  "main": "src/index.js",
  "scripts": {
